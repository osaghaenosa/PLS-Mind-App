{
  "expo": {
    "name": "PLS Mind",
    "slug": "FirstApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pls_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a68c37"
    },
    "ios": {
      "icon": "./assets/pls_logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.nosatoons.FirstApp"
    },
    "android": {
      "icon": "./assets/pls_logo.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "plsmind",
              "host": "open"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/pls_logo.png",
        "backgroundColor": "#a68c37"
      },
      "package": "com.nosatoons.FirstApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cec3d568-34ab-4592-b59e-149af4433f78"
      }
    }
  }
}
